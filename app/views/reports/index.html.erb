<h1>Reports#index</h1>
<p>Find me in app/views/reports/index.html.erb</p>
<%= form_tag("/reports/index", method: "get") do |f| %>
  <p>
    <%= select_date Date.today, prefix: :start_date %> -
    <%= select_date Date.today + 1.year, prefix: :end_date %> |
    <%= submit_tag "Change", :class => "btn btn-outlined btn-black" %>
